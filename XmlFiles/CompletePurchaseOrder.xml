<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Flow" parallel="tests" verbose="5">
    <test name="VerifyThatUnregisteredUserCanCompletepurchaseOrder">
        <parameter name="productName" value="mac"></parameter>
        <classes>
            <class name="CompletePurchase">
                <methods>
                    <include name="CompletePlaceOrder"></include>
                </methods>
            </class>
        </classes>
    </test>
</suite>
